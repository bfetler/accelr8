<%= render :partial => "accelerator_users/shared/links" %>

<h3>New Accelerator Login</h3>

<%= render :partial => "accelerator_users/shared/notices" %>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <p><%= f.label :name, "Accelerator Name" %><br />
  <%= f.text_field :name %></p>

  <p><%= f.label :email, "Accelerator Email" %><br />
  <%= f.email_field :email %></p>

  <p><%= f.label :password %><br />
  <%= f.password_field :password %></p>

  <p><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></p>

  <p><%= f.submit "Create New Accelerator Login" %></p>
<% end %>

