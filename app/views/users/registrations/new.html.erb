<%= render :partial => "users/shared/links" %>

<h3 class="usermargin">New User</h3>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <p class="usermargin"><%= f.label :email %><br />
  <%= f.email_field :email %></p>

  <p class="usermargin"><%= f.label :password %><br />
  <%= f.password_field :password %></p>

  <p class="usermargin"><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></p>

  <p class="usermargin"><%= f.submit "Create New User" %></p>
<% end %>
